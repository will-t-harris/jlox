// Simple inheritance
class Doughnut {
    cook() {
        print "Fry until golden brown.";
    }
}

class BostonCream < Doughnut {}

BostonCream().cook();

// Calling original superclass methods with `super`
class ChocolateDoughnut < Doughnut {
    cook() {
        super.cook();
        print "Coat with chocolate.";
    }
}

ChocolateDoughnut().cook();